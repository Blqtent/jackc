
class Math2 {
	function int shiftLeft(int value, int shift) {
		while (shift > 0) {
			let value = value * 2;
			let shift = shift - 1;
		}
		return value;
	}

	function int shiftRight(int value, int shift) {
		while (shift > 0) {
			let value = value / 2;
			let shift = shift - 1;
		}
		return value;
	}

	function int rem(int a, int b) {
		var int c;
		if (b = 0) {
			return 0;
		}
		let c = a / b;
		let c = c * b;
		return a - c;
	}
}

